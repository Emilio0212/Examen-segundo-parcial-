  <property name="styleSheet">
   <string notr="true">/* ==================== BASE GENERAL ==================== */
QWidget {
    background-color: #0b0c10; /* Fondo casi negro */
    color: #66fcf1; /* Azul neón claro */
    font-family: &quot;Orbitron&quot;, &quot;Segoe UI&quot;, sans-serif;
    font-size: 12pt;
}

/* ==================== FRAMES ==================== */
QFrame {
    background-color: #0b0c10;
    border: 1px solid #45a29e;
    border-radius: 10px;
}

QFrame#frame_catalogo {
    background-color: #0b0c10;
    border: 2px solid #45a29e;
    border-radius: 10px;
    box-shadow: 0 0 15px #45a29e;
}

/* ==================== BARRA SUPERIOR ==================== */
QFrame#frame_superior {
    background-color: #1f2833;
    border-bottom: 2px solid #45a29e;
}

QLabel#titulo_barra {
    color: #66fcf1;
    font-size: 16pt;
    font-weight: bold;
    letter-spacing: 1px;
}

/* ==================== BOTONES ==================== */
QPushButton {
    background-color: #1f2833;
    border: 2px solid #45a29e;
    border-radius: 8px;
    color: #66fcf1;
    padding: 6px 12px;
    font-weight: bold;
    text-transform: uppercase;
    letter-spacing: 1px;
    transition: all 0.2s ease-in-out;
}

QPushButton:hover {
    background-color: #0b0c10;
    border: 2px solid #66fcf1;
    box-shadow: 0 0 10px #66fcf1;
}

QPushButton:pressed {
    background-color: #45a29e;
    color: #0b0c10;
}

/* ==================== CAMPOS DE TEXTO ==================== */
QLineEdit {
    background-color: #0b0c10;
    border: 1px solid #45a29e;
    border-radius: 5px;
    padding: 4px 8px;
    color: #66fcf1;
}

QLineEdit:focus {
    border: 1px solid #66fcf1;
    box-shadow: 0 0 10px #66fcf1;
}

/* ==================== TABLAS ==================== */
QTableWidget {
    background-color: #0b0c10;
    color: #66fcf1;
    border: 1px solid #45a29e;
    gridline-color: #45a29e;
    selection-background-color: #45a29e;
    selection-color: #0b0c10;
}

QHeaderView::section {
    background-color: #1f2833;
    border: 1px solid #45a29e;
    color: #66fcf1;
    font-weight: bold;
}

/* ==================== SCROLLBAR ==================== */
QScrollBar:vertical {
    background: #1f2833;
    width: 10px;
    border-radius: 5px;
}
QScrollBar::handle:vertical {
    background: #45a29e;
    border-radius: 5px;
}
QScrollBar::handle:vertical:hover {
    background: #66fcf1;
}

/* ==================== EFECTO BRILLO (detalles TRON) ==================== */
QPushButton, QFrame, QLineEdit {
    outline: none;
}
</string>
